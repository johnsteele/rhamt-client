{
  "name": "rhamt-client",
  "version": "0.0.1",
  "description": "rhamt client",
  "keywords": [
    "rhamt",
    "windup",
    "migration",
    "jboss"
  ],
  "author": "John Steele",
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsteele/rhamt-client"
  },
  "engines": {
    "node": "*"
  },
  "main": "lib/main.js",
  "typings": "./lib/main",
  "devDependencies": {
    "@types/mocha": "^5.2.0",
    "@types/node": "^6.0.52",
    "mocha": "^5.2.0",
    "typescript": "~2.6.1",
    "nyc": "^11.0.2"
  },
  "scripts": {
    "build": "tsc -p .",
